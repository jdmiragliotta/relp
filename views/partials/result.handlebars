        <div><h2>{{business_category}}</h2></div>
        <div>
          <ul>
            <li><h3>Business Name: {{business_name}}</h3></li>
            <li><h3>Business Address: {{business_address1}}</h3></li>
            <li><h3>Business Address2: {{business_address2}}</h3></li>
            <li><h3>Business Phone Number: {{business_phone}}</h3></li>
            <li><h3>Business Rating: {{business_rating}}</h3></li>
            <li><h3>Business Comment: {{business_comment}}</h3></li>
          </ul>
        </div>
    {{/each}}
</div>